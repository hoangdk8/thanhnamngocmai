    @-webkit-keyframes particle-burst {
      0% {
        opacity: 0;
        -webkit-transform: translate(-50%, -50%)scale(0)rotate(0deg);
        transform: translate(-50%, -50%)scale(0)rotate(0deg)
      }

      15% {
        opacity: 1;
        -webkit-transform: translate(-50%, -50%)scale(1.3)rotate(var(--rot-start, 15deg));
        transform: translate(-50%, -50%)scale(1.3)rotate(var(--rot-start, 15deg))
      }

      100% {
        opacity: 0;
        -webkit-transform: translate(-webkit-calc(-50% + var(--dx)), -webkit-calc(-50% + var(--dy)))scale(.6)rotate(var(--rot-end, 180deg));
